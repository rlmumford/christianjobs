{
    "name": "christianjobs/christianjobs",
    "description": "ChristianJobs Website",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "rlmumford",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/youtube",
                "version": "2.1.10",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/youtube/releases/youtube_2.1.10.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/rlmumford/common.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:rlmumford/composer-patches.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:rlmumford/commerce_gocardless.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.com:freelygive/drupal/contacts_theme.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.com:freelygive/drupal/contacts-jobs-new.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.com:freelygive/drupal/contacts_jobs_apps.git"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "ckeditor-plugin/youtube": "^2.1",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "dev-relative-patches-1.x as 1.x-dev",
        "drupal/bootstrap": "^3.11",
        "drupal/ckeditor_youtube": "^1.1",
        "drupal/commerce": "^2.10",
        "drupal/commerce_gocardless": "dev-feature/redirect_flow as 1.x-dev",
        "drupal/commerce_paypal": "^1.0@beta",
        "drupal/communication": "^2",
        "drupal/contacts": "1.x-dev",
        "drupal/contacts_communication": "^1.0",
        "drupal/contacts_jobs": "dev-fmcg as 1.x-dev",
        "drupal/contacts_jobs_apps": "dev-fmcg as 1.x-dev",
        "drupal/contacts_notes": "^1.0",
        "drupal/contacts_theme": "1.x-dev",
        "drupal/core-composer-scaffold": "^8.9.13",
        "drupal/core-project-message": "^8.9.13",
        "drupal/core-recommended": "^8.9.13",
        "drupal/decoupled_auth": "^1.0@alpha",
        "drupal/elasticsearch_aws_connector": "6.x-dev",
        "drupal/elasticsearch_connector": "6.x",
        "drupal/email_registration": "^1.0@RC",
        "drupal/entity_route_context": "^1.1",
        "drupal/eu_cookie_compliance": "^1.2",
        "drupal/flag": "4.x-dev",
        "drupal/fontawesome": "^2.2",
        "drupal/geocoder": "^2.0@beta",
        "drupal/geofield": "^1.0@beta",
        "drupal/geofield_map": "^2.57",
        "drupal/google_analytics": "^2.3",
        "drupal/legal": "^1.0@beta",
        "drupal/mailgun": "^1.0@RC",
        "drupal/material_base": "^1.5",
        "drupal/metatag": "^1.7",
        "drupal/name": "1.x-dev",
        "drupal/og": "^1.0@alpha",
        "drupal/panelizer": "^4.1",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.3",
        "drupal/places": "1.0.x-dev",
        "drupal/profile": "^1.0@RC",
        "drupal/recaptcha": "^2.3",
        "drupal/redirect": "^1.3",
        "drupal/redis": "^1.5",
        "drupal/search_api": "^1.14",
        "drupal/search_api_location": "^1.0@alpha",
        "drupal/select2": "^1.0@beta",
        "drupal/seo_checklist": "^4.1",
        "drupal/social_link_field": "^1.1@alpha",
        "drupal/social_media": "^1.3",
        "drupal/time_field": "^1.11",
        "drupal/token": "^1.5",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drupal/yoast_seo": "^1.4",
        "drush/drush": "^9.0.0",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "rlmumford/common": "dev-master",
        "vlucas/phpdotenv": "^2.4",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/devel": "^1.2",
        "drupal/core-dev": "^8.9"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {},
    "extra": {
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "Field definition missing in block deriver": "./patches/field-block-deriver-missing-field-def.patch"
            },
            "drupal/social_link_field": {
                "Main Property Fix": "https://www.drupal.org/files/issues/2020-07-06/3157171-2.patch"
            },
            "drupal/inline_entity_form": {
                "Bundle selection fix": "https://www.drupal.org/files/issues/2019-08-20/3076005-2.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[project-root]/.gitattributes": {
                    "append": "scaffold/.gitattributes.txt"
                },
                "[web-root]/robots.txt": {
                    "append": "scaffold/robots.txt"
                }
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/modules/rlmumford": ["rlmumford/common"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "installer-types": ["bower-asset", "npm-asset"]
    }
}
